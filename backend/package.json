  "main": "index.js",
  "scripts": {
    "sls": "sls",
    "debug": "export SLS_DEBUG=* && cross-env NODE_OPTIONS=--max-old-space-size=8192 node --inspect node_modules/serverless/bin/serverless offline start -r us-west-1 --stage local --config serverless.js --reloadHandler",
    "start": "sls offline start -r us-west-2 --stage local --skipCacheInvalidation",
    "dev": "cross-env NODE_OPTIONS=--max-old-space-size=8192 sls offline start -r us-west-2 --stage local --config serverless.js --reloadHandler --noTimeout",
    "type-check": "tsc --noEmit",
    "lint-only": "eslint 'src/**/*.{js,ts}' --ignore-path .gitignore",
    "lint": "npm type-check && npm lint-only",
    "lint:fix": "npm lint-only --fix",
    "fixtures": "ts-node ./fixtures.ts",
    "typeorm": "ts-node --project ./tsconfig.json -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:run": "npm run typeorm migration:run -- -d ./src/lib/database/database.ts",
    "migration:create": "npm run typeorm migration:create $file_name",
    "migration:revert": "npm run typeorm migration:revert",
    "migration:generate": "npm run typeorm migration:generate -d ./src/lib/database/database.ts",
    "test": "jest -w 2",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watchAll",
    "test:only": "jest --watch -t",
    "precommit": "npm lint && npm test"
  },
